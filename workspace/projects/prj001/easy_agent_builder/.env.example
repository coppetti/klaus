# ===========================================
# Easy Agent Builder - Environment Variables
# ===========================================

# ===========================================
# GCP Configuration
# ===========================================
GOOGLE_CLOUD_PROJECT=seu-projeto-gcp
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Vertex AI
VERTEX_AI_STAGING_BUCKET=gs://seu-bucket-staging

# ===========================================
# Bibha.ai Integration
# ===========================================
BIBHA_API_KEY=sua-api-key-da-bibha
BIBHA_WEBHOOK_SECRET=seu-webhook-secret
BIBHA_WEBHOOK_URL=https://api.bibha.ai/webhooks/agent-response

# ===========================================
# Redis (Session Management)
# ===========================================
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# ===========================================
# Application
# ===========================================
ENVIRONMENT=development
LOG_LEVEL=INFO
PORT=8080

# Security
API_KEY_SECRET=sua-api-key-para-autenticacao
JWT_SECRET=seu-jwt-secret

# ===========================================
# Models
# ===========================================
DEFAULT_MODEL=gemini-2.0-flash-exp
FALLBACK_MODEL=gemini-1.5-pro

# ===========================================
# Observability
# ===========================================
ENABLE_TRACING=true
ENABLE_METRICS=true
TRACING_SAMPLE_RATE=0.1
