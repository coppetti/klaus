[run]
source = src/agent_builder
branch = True
omit = 
    */tests/*
    */test_*
    */venv/*
    */.venv/*
    */__pycache__/*
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

show_missing = True
skip_covered = False
fail_under = 70

[html]
directory = htmlcov

[xml]
output = coverage.xml
